Public Sub ToggleButton14_Click()
ToggleButton14.Text = TextBox48.Text

  If ToggleButton14.Value Then
    ToggleButton14.Value = True
    ToggleButton14.Background = Color.Green
'     Music.Play(User.home & "/MÃºsica/Balada/Interpretes de Baladas/Alex Ubago/Alex Ubago..Fantasia o Realidad/01-Track 1 - Alex Ubago.ogg")
    
    Else
      ToggleButton14.Value = False
      ToggleButton14.Background = Color.Yellow
  Endif
  

End

