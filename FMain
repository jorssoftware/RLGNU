Public Sub Timer1Hora_Timer()
  Dim unnombre As String

   Timer1Hora.Delay = 1000
Label3Hora.Text = Time(Hour(Now), Minute(Now), Second(Now))

 unnombre = "  Hora"
Label3Hora.text = Label3Hora.Text & unnombre & ""


' Label3Hora.Text = Time(Hour(19), Minute(50), Second(00), Label4Fecha.Foreground = Color.Blue)
End

